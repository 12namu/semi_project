<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <resource-ref>
    <res-ref-name>jdbc/myOracle</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
  <servlet>
    <servlet-name>auction</servlet-name>
    <servlet-class>com.fm.control.AuctionController</servlet-class>
    <init-param>
      <param-name>auctionConfig</param-name>
      <param-value>AuctionCommand.properties</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>auction</servlet-name>
    <url-pattern>*.ac</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>member</servlet-name>
    <servlet-class>com.fm.control.MemberController</servlet-class>
    <init-param>
      <param-name>memberConfig</param-name>
      <param-value>MemberCommand.properties</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>member</servlet-name>
    <url-pattern>*.me</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>review</servlet-name>
    <servlet-class>com.fm.control.ReviewController</servlet-class>
    <init-param>
      <param-name>reviewConfig</param-name>
      <param-value>ReviewCommand.properties</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>review</servlet-name>
    <url-pattern>*.rw</url-pattern>
  </servlet-mapping>
</web-app>